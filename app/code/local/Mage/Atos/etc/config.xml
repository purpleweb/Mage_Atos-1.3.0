<config>
    <modules>
        <Mage_Atos>
            <version>1.3.0</version>
        </Mage_Atos>
    </modules>
    <global>
        <models>
            <atos>
                <class>Mage_Atos_Model</class>
                <resourceModel>atos_mysql4</resourceModel>
            </atos>
            <atos_mysql4>
                <class>Mage_Atos_Model_Mysql4</class>
            </atos_mysql4>
        </models>
        <blocks>
            <atos>
                <class>Mage_Atos_Block</class>
            </atos>
        </blocks>
        <helpers>
            <atos>
                <class>Mage_Atos_Helper</class>
            </atos>
        </helpers>
        <resources>
            <atos_setup>
                <setup>
                    <module>Mage_Atos</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </atos_setup>
            <atos_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </atos_write>
            <atos_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </atos_read>
        </resources>
        <payment>
            <atos_standard>
                <!-- Liste des langages supportées -->
                <languages>
                    <fr>
                        <code>fr</code>
                        <name>Français</name>
                    </fr>
                    <en>
                        <code>en</code>
                        <name>Anglais</name>
                    </en>
                    <sp>
                        <code>sp</code>
                        <name>Espagnol</name>
                    </sp>
                    <ge>
                        <code>ge</code>
                        <name>Allemand</name>
                    </ge>
                    <it>
                        <code>it</code>
                        <name>Italien</name>
                    </it>
                </languages>
                <!-- Liste des cartes de crédit supportées -->
                <credit_card>
                    <CB>
                        <code>CB</code>
                        <name>Carte Bleue</name>
                    </CB>
                    <MASTERCARD>
                        <code>MASTERCARD</code>
                        <name>MasterCard</name>
                    </MASTERCARD>
                    <VISA>
                        <code>VISA</code>
                        <name>Visa</name>
                    </VISA>
                    <AMEX>
                        <code>AMEX</code>
                        <name>American Express</name>
                    </AMEX>
                    <DINERS>
                        <code>DINERS</code>
                        <name>Diner's club</name>
                    </DINERS>
                    <JCB>
                        <code>JCB</code>
                        <name>Japanese Credit Bureau (JCB)</name>
                    </JCB>
                    <COFINOGA>
                        <code>COFINOGA</code>
                        <name>Cofinoga</name>
                    </COFINOGA>
                    <FINAREF>
                        <code>FINAREF</code>
                        <name>Finaref</name>
                    </FINAREF>
                </credit_card>
                <!-- Liste des devises supportées -->
                <currencies>
                    <EUR>
                        <code>978</code>
                        <iso>EUR</iso>
                        <name>Euro</name>
                    </EUR>
                    <USD>
                        <code>840</code>
                        <iso>USD</iso>
                        <name>Dollar US</name>
                    </USD>
                    <CHF>
                        <code>756</code>
                        <iso>CHF</iso>
                        <name>Franc Suisse</name>
                    </CHF>
                    <GBP>
                        <code>826</code>
                        <iso>GBP</iso>
                        <name>Livre Sterling</name>
                    </GBP>
                    <CAD>
                        <code>124</code>
                        <iso>CAD</iso>
                        <name>Dollar Canadien</name>
                    </CAD>
                    <JPY>
                        <code>392</code>
                        <iso>JPY</iso>
                        <name>Yen japonais</name>
                    </JPY>
                    <MXP>
                        <code>484</code>
                        <iso>MXP</iso>
                        <name>Peso mexicain</name>
                    </MXP>
                    <TRL>
                        <code>792</code>
                        <iso>TRL</iso>
                        <name>Ancienne lire turque</name>
                    </TRL>
                    <AUD>
                        <code>036</code>
                        <iso>AUD</iso>
                        <name>Dollar australien</name>
                    </AUD>
                    <NZD>
                        <code>554</code>
                        <iso>NZD</iso>
                        <name>Dollar néo-zélandais</name>
                    </NZD>
                    <NOK>
                        <code>578</code>
                        <iso>NOK</iso>
