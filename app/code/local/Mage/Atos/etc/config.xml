                    </NOK>
                    <BRC>
                        <code>986</code>
                        <iso>BRC</iso>
                        <name>Cruzeiro Brésilien</name>
                    </BRC>
                    <ARP>
                        <code>032</code>
                        <iso>ARP</iso>
                        <name>Peso argentin</name>
                    </ARP>
                    <KHR>
                        <code>116</code>
                        <iso>KHR</iso>
                        <name>Riel Cambodge</name>
                    </KHR>
                    <TWD>
                        <code>901</code>
                        <iso>TWD</iso>
                        <name>Dollar de Taiwan</name>
                    </TWD>
                    <DKK>
                        <code>208</code>
                        <iso>DKK</iso>
                        <name>Couronne danoise</name>
                    </DKK>
					<CZK>
						<code>203</code>
						<iso>CZK</iso>
						<name>Couronne tchèque</name>
					</CZK>
                    <SEK>
                        <code>752</code>
                        <iso>SEK</iso>
                        <name>Couronne Suédoise</name>
                    </SEK>
                    <KRW>
                        <code>410</code>
                        <iso>KRW</iso>
